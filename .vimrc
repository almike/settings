syntax enable
color jellybeans
set backupdir=/tmp/
set directory=/tmp/
set laststatus=2
set shiftwidth=2
set tabstop=2
set t_Co=256
set so=16
set nu
set hlsearch
set smartcase
set autoread
set autoindent
set binary
set noeol
set list
set wildignore+=public/**,tmp/**,node_modules/**,coverage/**
let g:indent_guides_enable_on_vim_startup = 1
execute pathogen#infect()
"sudo npm install -g jshint
map <C-N> :cnext<enter>
map <C-P> :cprev<enter>
nmap :E :e **/
nmap :T :tabedit **/
nmap :W :wa<Enter>
imap jj <Esc>
hi IndentGuidesOdd ctermbg=234
hi IndentGuidesEven ctermbg=235
hi NonText ctermbg=none
hi Special ctermbg=none
hi SpecialKey ctermbg=none
hi LineNr ctermbg=none
hi Normal ctermbg=none
au BufNewFile,BufRead */mickael/rsa/* set expandtab
au BufNewFile,BufRead *.xea setlocal ft=javascript
au CursorHold * checktime
au CursorMoved * exe printf('match DiffChange /\V\<%s\>/', escape(expand('<cword>'), '/\'))